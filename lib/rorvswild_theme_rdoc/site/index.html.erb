    <main class="container">
      <section>
        <ul class="gems-list">
          <% for project in @projects %>
            <li class="gem">
              <% if project.docs.size > 1 %>
                <details>
                  <summary>
                    <hgroup>
                      <h2 class="gem__title">
                        <a href="<%= project.docs.last.url %>"><%= project.url %></a>
                      </h2>
                      <p class="gem__about"><%= project.summary %></p>
                    </hgroup>
                  </summary>
                  <ul>
                    <% for doc in project.docs.reverse %>
                      <li>
                        <a href="<%= doc.url %>">
                          <span class="version"><%= doc.version.number %></span>
                        </a>
                      </li>
                    <% end %>
                  </ul>
                </details>
              <% else %>
                <hgroup>
                  <h2 class="gem__title">
                    <a href="<%= project.docs.first.url %>"><%= project.url %></a>
                  </h2>
                  <p class="gem__about"><%= project.summary %></p>
                </hgroup>
              <% end %>
            </li>
          <% end %>
        </ul>
      </section>

